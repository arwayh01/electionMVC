                                    </div>
                                </header>
                                <div class="content-body">    <div class="row">
                                        <div class="col-lg-10 col-md-10 col-10 offset-lg-1 offset-md-1 offset-1">
                                            <canvas id="radar-chartjs" width="254" height="254" style="width: 254px; height: 254px;"></canvas>
                                        </div>
                                    </div>
                                </div>
                            </section></div>			
                    </div>


                    <!-- MAIN CONTENT AREA ENDS -->
                </section>

  
<!-- Morris.js charts -->
   <script src="https://ajax.googleapis.com/ajax/libs/jquery/3.5.1/jquery.min.js"></script>
<script src="~/assets/plugins/morris-chart/js/raphael-min.js" type="text/javascript"></script>
        <script src="~/assets/plugins/morris-chart/js/morris.min.js" type="text/javascript"></script>
<script>
  $(function () {
    "use strict";
    //DONUT CHART
    var donut = new Morris.Donut({
      element: 'sales-chart',
      resize: true,
      colors: ["#3c8dbc", "#f56954", "#00a65a"],
      data: [
        {label: "Registered Doctors", value: @Model.doctors_count},
        {label: "Registered Nurses", value: @Model.nurses_count},
        {label: "Registered Patients", value: @Model.patients_count}
      ],
      hideHover: 'auto'
    });

     var donut = new Morris.Donut({
      element: 'genre-chart',
      resize: true,
      colors: ["#3c8dbc", "#f56954", "#00a65a"],
      data: [
        {label: "Homme", value: @Model.doctors_count},
        {label: "Femme", value: @Model.nurses_count}
     
      ],
      hideHover: 'auto'
    });
        var donut = new Morris.Donut({
      element: 'centre-chart',
      resize: true,
      colors: ["#3c8dbc", "#f56954", "#00a65a"],
      data: [
        {label: "Registered Doctors", value: @Model.doctors_count},
        {label: "Registered Nurses", value: @Model.nurses_count},
        {label: "Registered Patients", value: @Model.patients_count}
      ],
      hideHover: 'auto'
    });
            var donut = new Morris.Donut({
      element: 'ville-chart',
      resize: true,
      colors: ["#3c8dbc", "#f56954", "#00a65a"],
      data: [
        {label: "Registered Doctors", value: @Model.doctors_count},
        {label: "Registered Nurses", value: @Model.nurses_count},
        {label: "Registered Patients", value: @Model.patients_count}
      ],
      hideHover: 'auto'
    });
  });
</script>
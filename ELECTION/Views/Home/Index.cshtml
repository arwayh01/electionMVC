@model ELECTION.Models.ModelDashboardView
  <section class="wrapper main-wrapper row" style="">

                    <div class="col-12">
                        <div class="page-title">

                            <div class="float-left">
                                <!-- PAGE HEADING TAG - START --><h1 class="title">Visitor Reports</h1><!-- PAGE HEADING TAG - END -->                            </div>

                            <div class="float-right d-none">
                                <ol class="breadcrumb">
                                    <li>
                                        <a href="index.html"><i class="fa fa-home"></i>Home</a>
                                    </li>
                                    <li>
                                        <a href="blo-report-site.html">Reports</a>
                                    </li>
                                    <li class="active">
                                        <strong>Visitors</strong>
                                    </li>
                                </ol>
                            </div>

                        </div>
                    </div>
                    <div class="clearfix"></div>
                    <!-- MAIN CONTENT AREA STARTS -->
                    <div class="row margin-0">
                        <div class="col-12 col-lg-6">
                            <section class="box ">
                                <header class="panel_header">
                                    <h2 class="title float-left">Par Ville</h2>
                                    <div class="actions panel_actions float-right">
                                        <a class="box_toggle fa fa-chevron-down"></a>
                                        <a class="box_setting fa fa-cog" data-toggle="modal" href="#section-settings"></a>
                                        <a class="box_close fa fa-times"></a>
                                    </div>
                                </header>
                                <div class="content-body">    <div class="row">
                                        <div class="col-12">
                                           <div class="chart" id="sales-chart" style="height: 300px; position: relative;"></div>
                                        </div>
                                    </div>
                                </div>
                            </section></div>

                        <div class="col-12 col-lg-6">
                            <section class="box ">
                                <header class="panel_header">
                                    <h2 class="title float-left">Par Centre</h2>
                                    <div class="actions panel_actions float-right">
                                        <a class="box_toggle fa fa-chevron-down"></a>
                                        <a class="box_setting fa fa-cog" data-toggle="modal" href="#section-settings"></a>
                                        <a class="box_close fa fa-times"></a>
                                    </div>
                                </header>
                                <div class="content-body">    <div class="row">
                                        <div class="col-12">
                                            <div class="chart" id="centre-chart" style="height: 300px; position: relative;"></div>
                                        </div>
                                    </div>
                                </div>
                            </section></div>


                        <div class="col-12 col-lg-6">
                            <section class="box ">
                                <header class="panel_header">
                                    <h2 class="title float-left">Par Ville</h2>
                                    <div class="actions panel_actions float-right">
                                        <a class="box_toggle fa fa-chevron-down"></a>
                                        <a class="box_setting fa fa-cog" data-toggle="modal" href="#section-settings"></a>
                                        <a class="box_close fa fa-times"></a>
                                    </div>
                                </header>
                                <div class="content-body">    <div class="row">
                                      <div class="col-12">
                                            <div class="chart" id="ville-chart" style="height: 300px; position: relative;"></div>
                                        </div>
                                    </div>
                                </div>
                            </section></div>

                        <div class="col-12 col-lg-6">
                            <section class="box ">
                                <header class="panel_header">
                                    <h2 class="title float-left">Par Genre</h2>
                                    <div class="actions panel_actions float-right">
                                        <a class="box_toggle fa fa-chevron-down"></a>
                                        <a class="box_setting fa fa-cog" data-toggle="modal" href="#section-settings"></a>
                                        <a class="box_close fa fa-times"></a>
                                    </div>
                                </header>
                                <div class="content-body">    <div class="row">
                                        <div class="col-12">
                                            <div class="chart" id="genre-chart" style="height: 300px; position: relative;"></div>
                                        </div>
                                    </div>
                                </div>
                            </section></div>

                        <div class="clearfix"></div>

                        <div class="col-12 col-lg-6">
                            <section class="box ">
                                <header class="panel_header">
                                    <h2 class="title float-left">Polar chart</h2>
                                    <div class="actions panel_actions float-right">
                                        <a class="box_toggle fa fa-chevron-down"></a>
                                        <a class="box_setting fa fa-cog" data-toggle="modal" href="#section-settings"></a>
                                        <a class="box_close fa fa-times"></a>
                                    </div>
                                </header>
                                <div class="content-body">    <div class="row">
                                        <div class="col-lg-10 col-md-10 col-10 offset-lg-1 offset-md-1 offset-1">
                                            <div class="chart" id="sales-chart" style="height: 300px; position: relative;"></div>
                                        </div>
                                    </div>
                                </div>
                            </section></div>			

                        <div class="col-12 col-lg-6">
                            <section class="box ">
                                <header class="panel_header">
                                    <h2 class="title float-left">Radar chart</h2>
